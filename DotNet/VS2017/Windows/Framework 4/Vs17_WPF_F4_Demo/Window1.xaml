                  <Grid>
                     <Button HorizontalAlignment="Left" Margin="10,10,0,0" Name="ButListenerInit" TabIndex="32" Width="64" Height="23" VerticalAlignment="Top" Click="butListenerInit_Click">Initialize</Button>
